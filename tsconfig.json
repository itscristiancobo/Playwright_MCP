{
  "compilerOptions": {
    "baseUrl": "./Playwright",
    "target": "es2021",
    "module": "commonjs",
    "strict": true,
    "noEmit": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "paths": {
      "@pages/*": ["pages/*"]
    }
  },
  "include": ["**/*.ts"]
}
